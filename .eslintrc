{
  "env": {
    "browser": false,
    "node": true,
	  "amd": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,

    "accessor-pairs": [1, {"getWithoutSet": false, "setWithoutGet": true}],
    "block-scoped-var": 1,
	  "consistent-return": 1,
	  "curly": 2,
	  "default-case": 1,
	  "dot-location": [2, "property"],
	  "dot-notation": [1, {"allowKeywords": true}],
	  "eqeqeq": 2,
	  "guard-for-in": 0,
	  "no-alert": 2,
	  "no-caller": 2,
	  "no-case-declarations": 1,
	  "no-div-regex": 1,
	  "no-else-return": 2,
	  "no-empty-pattern": 2,
	  "no-eq-null": 2,
	  "no-eval": 2,
	  "no-extend-native": 2,
	  "no-extra-bind": 1,
	  "no-fallthrough": 1,
	  "no-floating-decimal": 2,
	  "no-implicit-coercion": 1,
	  "no-implicit-globals": 0,
	  "no-implied-eval": 2,
	  "no-invalid-this": 2,
	  "no-iterator": 1,
	  "no-labels": 2,
	  "no-lone-blocks": 1,
	  "no-loop-func": 1,
	  "no-multi-spaces": 2,
	  "no-multi-str": 1,
	  "no-native-reassign": 2,
	  "no-new-func": 2,
	  "no-new-wrappers": 2,
	  "no-new": 2,
	  "no-octal-escape": 2,
	  "no-octal": 1,
	  "no-param-reassign": 1,
	  "no-process-env": 0,
	  "no-proto": 2,
	  "no-redeclare": 2,
	  "no-return-assign": 2,
	  "no-script-url": 2,
	  "no-self-compare": 2,
	  "no-sequences": 1,
	  "no-throw-literal": 1,
	  "no-unmodified-loop-condition": 0,
	  "no-unused-expressions": [2, {"allowShortCircuit": true, "allowTernary": true}],
	  "no-useless-call": 1,
	  "no-useless-concat": 1,
	  "no-void": 2,
	  "no-warning-comments": 0,
	  "no-with": 2,
	  "vars-on-top": 0,
	  "wrap-iife": [2, "inside"],
	  "yoda": 2,

	  "strict": 0,

	  "no-catch-shadow": 1,
	  "no-delete-var": 2,
	  "no-label-var": 2,
	  "no-shadow-restricted-names": 2,
	  "no-shadow": 1,
	  "no-undef-init": 1,
	  "no-undef": 2,
	  "no-undefined": 2,
	  "no-unused-vars": 2,
	  "no-use-before-define": 1,

	  "callback-return": 1,
	  "global-require": 2,
	  "handle-callback-err": 1,
	  "no-mixed-requires": 2,
	  "no-new-require": 2,
	  "no-path-concat": 2,
	  "no-process-exit": 2,
	  "no-restricted-imports": 0,
	  "no-restricted-modules": 0,
	  "no-sync": 1,

    "array-bracket-spacing": [2, "never"],
    "block-spacing": [2, "always"],
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "camelcase": [2, {"properties": "always"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "computed-property-spacing": [2, "never"],
    "consistent-this": [2, "that"],
    "eol-last": 2,
    "func-names": 0,
    "func-style": [2, "declaration", { "allowArrowFunctions": true }],
    "id-length": 0,
    "indent": [2, 2],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "keyword-spacing": 2,
    "linebreak-style": [2, "unix"],
    "lines-around-comment": [2, {
      "beforeBlockComment": true,
      "afterBlockComment": false,
      "beforeLineComment": true,
      "afterLineComment": false,
      "allowBlockStart": true,
      "allowBlockEnd": true
    }],
    "max-depth": [1, 4],
    "max-len": [2, 120, 2, {
      "ignoreComments": false,
      "ignoreUrls": true
    }],
    "max-nested-callbacks": 0,
    "max-params": [1, 5],
    "max-statements": 0,
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-negated-condition": 0,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-plusplus": 0,
    "no-whitespace-before-property": 2,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2, "never"],
    "one-var": 0,
    "operator-assignment": [1, "always"],
    "operator-linebreak": [1, "before"],
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed", {"keywords": true}],
    "quotes": [2, "single", "avoid-escape"],
    "require-jsdoc": 0,
    "semi-spacing": [2, {"before": false, "after": true}],
    "semi": [2, "always"],
    "sort-imports": [2, {"memberSyntaxSortOrder": ["none", "all", "single", "multiple"]}],
    "sort-vars": [2, {"ignoreCase": false, }],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 1,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": [2, "always", {"exceptions": ["*"]}],
    "wrap-regex": 0,

    "arrow-body-style": [2, "always"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": [2, {"before": true, "after": true}],
    "constructor-super": 2,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "no-class-assign": 2,
    "no-confusing-arrow": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-constructor": 2,
    "no-var": 1,
    "object-shorthand": [1, "never"],
    "prefer-arrow-callback": 2,
    "prefer-const": 1,
    "prefer-reflect": [2, {"exceptions": ["delete"]}],
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-yield": 1,
    "yield-star-spacing": [2, "after"]

    # Documented but not found:
    # keyword-spacing, max-cap, max-parens, no-whitespace-before-property,
    # no-confusing-arrow, no-useless-constructor
  }
}
